{
	"name": "cf-worker-redirect",
	"version": "1.0.1",
	"description": "Simple URL shortener running at the edge with Cloudflare Workers and Worker KV.",
	"main": "index.js",
	"scripts": {
		"lint": "eslint ./src/ --ext .ts",
		"test:clean": "rimraf ./transpiled/src ./transpiled/test",
		"test": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled/test",
		"transpile": "tsc --project ./test",
		"dev": "wrangler dev",
		"build": "webpack"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/betahuhn/cf-worker-redirect.git"
	},
	"keywords": [
		"template",
		"workers",
		"cloudflare-workers",
		"typescript",
		"nodejs"
	],
	"author": "Maximilian Schiller <schiller@mxis.ch>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/betahuhn/cf-worker-redirect/issues"
	},
	"homepage": "https://github.com/betahuhn/cf-worker-redirect#readme",
	"devDependencies": {
		"@cloudflare/workers-types": "^2.2.1",
		"@cloudflare/wrangler": "^1.15.1",
		"@types/chai": "^4.2.16",
		"@types/mocha": "^8.2.2",
		"chai": "^4.3.4",
		"mocha": "^8.3.2",
		"rimraf": "^3.0.2",
		"service-worker-mock": "^2.0.5",
		"ts-loader": "^8.1.0",
		"typescript": "^4.2.4",
		"webpack": "^5.33.2",
		"webpack-cli": "^4.6.0"
	},
	"dependencies": {
		"@betahuhn/config": "^1.1.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"eslint": "^7.23.0"
	}
